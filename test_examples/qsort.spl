program quick_sort;
const
  start = 65;
  end = 68;
type
  score = record
    id: char;
    points: array [1..3] of real;
  end;
  data = array [start..end] of score;
var
  data_set: data;

function small_than(a, b: score): boolean;
begin
  small_than = a.points[1] < b.points[1];
end;

procedure qsort(l, r:integer; var a: data);
var
  i, j: integer;
  x, t: score;
begin
  i = l;
  j = r;
  x = a[(i + j) div 2];

  repeat
    while small_than(x, a[j]) do j := j - 1;
    while small_than(a[i], x) do i := i + 1;
    if i < j then begin
      t := a[i];
      a[i] := a[j];
      a[j] := t;
    end;
  until i >= j;

  if i < r then qsort(i, r, a);
  if l < j then qsort(l, j, a);
end;

begin
  for i := start to end do begin
    data_set[i].id = chr(i);
    data_set[i].points[1] = 100-i;
    data_set[i].points[2] = 0;
    data_set[i].points[3] = 0;
  end;
  qsort(start, end, data_set);
  for i := start to end do begin
    write(data_set[i].id, ' ', data_set[i].points[1]);
    writeln;
  end;
end.